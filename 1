#include <stdio.h>
int main()
{
	int data[]={1,2,3,4};
	int q[100];
	int temp=0;
	for(int i=1;i<=4;i++)
	{
		for(int j=1;j<=4;j++)	
		{
			if(i==j)
			continue;
			for(int z=1;z<=4;z++)
			{
				if(z==i||z==j)
				continue;
				q[temp]=i*100+j*10+z;
				temp++;
			}
		}
	}
	for(int i=0;i<=temp;i++)
	{
		printf("%d\r\n",q[i]);	
	}
	printf("共有%d个数\r\n",temp);	
	return 0;
}
